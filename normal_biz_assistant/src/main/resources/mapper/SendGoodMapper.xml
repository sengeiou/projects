<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.normal.bizassistant.autosend.SendGoodMapper">
  <resultMap id="BaseResultMap" type="com.normal.bizassistant.autosend.SendGood">
    <id column="id" jdbcType="INTEGER" property="id" />
    <result column="category_id" jdbcType="VARCHAR" property="categoryId" />
    <result column="text" jdbcType="VARCHAR" property="text" />
    <result column="image_paths" jdbcType="VARCHAR" property="imagePaths" typeHandler="com.normal.bizassistant.autosend.ImagePathTypeHandler"/>
  </resultMap>
  <sql id="Base_Column_List">
    id, post_title, post_preview, update_time
  </sql>

  <insert id="batchInsert">
    insert into trd_send_good (category_id, text, image_paths)
     values
    <foreach collection="goods" item="item" separator=",">
      (#{item.categoryId}, #{item.text}, #{imageStrs})
    </foreach>
  </insert>


</mapper>